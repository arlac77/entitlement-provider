{
  "name": "entitlement-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/entitlement-provider-cli.mjs",
  "description": "provide entitlements via OIDC",
  "keywords": [
    "OIDC"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "entitlement-provider": "bin/entitlement-provider"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/entitlement-provider-cli.mjs --section=API",
    "lint": "documentation lint src/entitlement-provider-cli.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "config-expander": "^10.2.3",
    "koa": "^2.7.0",
    "koa-ejs": "^4.2.0",
    "koa-helmet": "^4.1.0",
    "koa-mount": "^4.0.0",
    "oidc-provider": "^5.5.5"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "c8": "^3.5.0",
    "documentation": "^10.1.0",
    "esm": "^3.2.22",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.10.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-executable": "^1.4.5",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "semantic-release": "^15.13.12"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/entitlement-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/entitlement-provider/issues"
  },
  "homepage": "https://github.com/arlac77/entitlement-provider#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
